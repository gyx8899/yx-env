{
  "name": "yx-env",
  "version": "0.1.1",
  "description": "A personal scripts in Node/Shell/Dos for projects",
  "scripts": {
    "git:push": "git-push.sh",
    "git:pull-rebase": "git-pull-rebase.sh",
    "git:global-user": "git-global-user.sh",
    "git:local-user": "git-local-user.sh",
    "git:registry-taobao": "git-registry-taobao.sh",
    "git:remote-template": "git-remote-template.sh",
    "node: nvm-12": "nvm use 12.16.1",
    "view-port-80": "dos-netstat-80.bat",
    "node: run js": "node assets/scripts/run-test.js",
    "node: run test": "node-es6.sh run-test param1 param2",
    "node-param: set": "set arg1=123 & set arg2=456 & node ./assets/scripts/node-param.js & set argn=收不到或者无效",
    "node-param: cross-env": "cross-env arg1=123 arg2=456 node ./assets/scripts/node-param.js argn=收不到或者无效",
    "node-param: yargs": "node ./assets/scripts/node-param.js --arg1 123 --arg2 456 -a 789 -a 2 -a 3 --argn 收到且有效"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gyx8899/yx-env.git"
  },
  "keywords": [
    "YX",
    "node"
  ],
  "author": "Kate Kuo",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "cross-env": "^7.0.3",
    "yargs": "^17.0.1"
  }
}
